<% resources.each do |res| %>
	<% if res.class.name == 'Presentation' %>
		<%= render 'presentations/presentation_mini', :presentation => res %>
	<% elsif res.class.name == 'Webinar' %>
		<%= render 'webinars/webinar_mini', :webinar => res %>
	<% elsif res.class.name == 'Recording' %>
		<%= render 'recordings/recording_mini', :recording => res %>
	<% else %>
		<%= render 'rs_items/rs_publication_mini', :item => res %>
	<% end %>
<% end %>